<%- include('partials/header') %>
<section class="bg-accent py-4">
    <div id="carouselExampleCaptions" class="carousel slide" data-bs-ride="carousel">
        <div class="carousel-indicators mb-5">
            <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="0" class="active"
                aria-current="true" aria-label="Slide 1"></button>
            <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="1"
                aria-label="Slide 2"></button>
            <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="2"
                aria-label="Slide 3"></button>
        </div>
        <div class="carousel-inner">
            <div class="carousel-item active">
                <div class="pb-lg-5">
                    <div class="container py-sm-4 my-lg-4 px-4 pb-md-4">
                        <div class="row align-items-center justify-content-center">
                            <div class="col-lg-5 col-md-6 order-md-2">

                                <img src="img/04.jpg" class="d-block w-100 mx-auto" alt="...">
                            </div>
                            <div
                                class="col-lg-5 col-md-6 offset-lg-1 order-md-1 pt-4 pb-md-4 text-center text-md-start">


                                <h2 class="fw-light pb-1 from-bottom">World of music with</h2>
                                <h1 class="display-4 from-bottom delay-1">Headphones</h1>
                                <h5 class="fw-light pb-3 from-bottom delay-2">Choose between top brands</h5>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="carousel-item">
                <div class="pb-lg-5">
                    <div class="container py-sm-4 my-lg-4 px-4 pb-md-4">
                        <div class="row align-items-center justify-content-center">
                            <div class="col-lg-5 col-md-6 order-md-2">
                                <img src="img/05.jpg" class="d-block w-100 mx-auto" alt="...">
                            </div>
                            <div
                                class="col-lg-5 col-md-6 offset-lg-1 order-md-1 pt-4 pb-md-4 text-center text-md-start">
                                <h2 class="fw-light pb-1 from-bottom">World of music with</h2>
                                <h1 class="display-4 from-bottom delay-1">Headphones</h1>
                                <h5 class="fw-light pb-3 from-bottom delay-2">Choose between top brands</h5>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="carousel-item">
                <div class="pb-lg-5">
                    <div class="container py-sm-4 my-lg-4 px-4 pb-md-4">
                        <div class="row align-items-center justify-content-center">
                            <div class="col-lg-5 col-md-6 order-md-2">
                                <img src="img/06.jpg" class="d-block w-100 mx-auto" alt="...">
                            </div>
                            <div
                                class="col-lg-5 col-md-6 offset-lg-1 order-md-1 pt-4 pb-md-4 text-center text-md-start">
                                <h2 class="fw-light pb-1 from-bottom">World of music with</h2>
                                <h1 class="display-4 from-bottom delay-1">Headphones</h1>
                                <h5 class="fw-light pb-3 from-bottom delay-2">Choose between top brands</h5>
                            </div>
                        </div>
                    </div>
                </div>
            </div>


        </div>
        <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleCaptions"
            data-bs-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Previous</span>
        </button>
        <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleCaptions"
            data-bs-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Next</span>
        </button>
    </div>

</section>
<!-- section product -->
<section class="container-fluid pt-5">
    <div class="d-flex flex-wrap justify-content-between align-items-center pt-1 border-bottom pb-4 mb-4">
        <h2 class="h3 mb-0 pt-3 me-2">Trending products</h2>
        <div class="pt-3">
            <a class="btn btn-outline-secondary" href="shop-grid-ls.html">More products
                <i class="ci-arrow-right ms-1 me-n1"></i>
            </a>
        </div>
    </div>
    <div class="row mx-n3">
        <div class="row row-cols-1 row-cols-sm-2 g-4 mb-2">
            <% theData.forEach(data => { %>
            <div class="col-lg-3 col-md-4">
              <div class="card border-0 shadow-sm">
                <div class="img-warp d-flex justify-content-center">
                  <img src="<%= data.image %> " class="card-img-top img-thumbnail border-0" alt="...">
                </div>
                <figcaption>
        
                </figcaption>
                <div class="card-body">
                  <h5 class="card-title text-truncate"><%= data.title %></h5>
                  <p class="card-text text-muted fs-6"><span><%= data.category %></span></p>
                  <p class="card-text">Price <span><%= data.price %></span></p>
                </div>
              </div>
            </div>
            <% }); %>
          </div>
    </div>
</section>
<%- include('partials/footer') %>